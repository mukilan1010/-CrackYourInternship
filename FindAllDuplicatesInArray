class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        ArrayList<Integer> l=new ArrayList<>();

        for(int i=0;i<nums.length;i++){
            int n=Math.abs(nums[i]);
            if(nums[n-1]<0){
                l.add(n);
            }
            nums[n-1]=nums[n-1]*-1;
        }
        return l;

        
    }
}
